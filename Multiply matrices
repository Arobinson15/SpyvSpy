         ;
         console.log(document.write( + g * n + h * r + i * v + ','));
         console.log(document.write( + g * o + h * s + i * w + ','));
         console.log(document.write( + g * p + h * t + i * x + ','))
         ;
         console.log(document.write( + j * m + k * p + l * u + ','));
         console.log(document.write( + j * n + k * r + l * v + ','));
         console.log(document.write( + j * o + k * s + l * w + ','))
         ;
         console.log(document.write( + j * p + k * t + l * x + ','));

      }

      // multiply 4x4 & 4x3
      else if(rowA == 4 && columnA == 4 && rowB == 4 && columnB == 3)
      {
         var a = parseInt(prompt());
         var b = parseInt(prompt());
         var c = parseInt(prompt());
         var  d = parseInt(prompt());
         var e = parseInt(prompt());
         var f = parseInt(prompt());
         var g = parseInt(prompt());
         var h = parseInt(prompt());
         var i = parseInt(prompt());
         var j = parseInt(prompt());
         var k = parseInt(prompt());
         var l = parseInt(prompt());
         //
         var m = parseInt(prompt());
         var n = parseInt(prompt());
         var o = parseInt(prompt());
         var p = parseInt(prompt());
         var q = parseInt(prompt());
         var r = parseInt(prompt());
         var s = parseInt(prompt());
         var t = parseInt(prompt());
         var u = parseInt(prompt());
         var v = parseInt(prompt());
         var w = parseInt(prompt());
         var x = parseInt(prompt());
         var y = parseInt(prompt());
         var z = parseInt(prompt());
         var aa = parseInt(prompt());
         var ba = parseInt(prompt());



         document.write("product of matrices= ");
         console.log(document.write( + a * q + b * t + c * w + d * z + ','));
         console.log(document.write( + a * r + b * u + c * x + d * aa + ','));
         console.log(document.write( + a * s + b * v + c * y + d * ba + ','));
         ;
         console.log(document.write( + e * q + f * t + g * w + h * z + ','));
         console.log(document.write( + e * r + f * u + g * x + h * aa + ','));
         console.log(document.write( + e * s + f * v + g * y + h * ba + ','));
         ;
         console.log(document.write( + i * q + j * t + k * w + l * z + ','));
         console.log(document.write( + i * r + j * u + k * x + l * aa + ','));
         console.log(document.write( + i * s + j * v + k * y + l * ba + ','));
         ;
         console.log(document.write( + m * q + n * t + o * w + p * z + ','));
         console.log(document.write( + m * r + n * u + o * x + p * aa + ','));
         console.log(document.write( + m * s + n * v + o * y + p * ba + ','));

      }


      // multiply 3x4 & 4x4
      else if(rowA == 3 && columnA == 4 && rowB == 4 && columnB == 4)
      {
         var a = parseInt(prompt());
         var b = parseInt(prompt());
         var c = parseInt(prompt());
         var  d = parseInt(prompt());
         var e = parseInt(prompt());
         var f = parseInt(prompt());
         var g = parseInt(prompt());
         var h = parseInt(prompt());
         var i = parseInt(prompt());
         var j = parseInt(prompt());
         var k = parseInt(prompt());
         var l = parseInt(prompt());
         //
         var m = parseInt(prompt());
         var n = parseInt(prompt());
         var o = parseInt(prompt());
         var p = parseInt(prompt());
         var q = parseInt(prompt());
         var r = parseInt(prompt());
         var s = parseInt(prompt());
         var t = parseInt(prompt());
         var u = parseInt(prompt());
         var v = parseInt(prompt());
         var w = parseInt(prompt());
         var x = parseInt(prompt());
         var y = parseInt(prompt());
         var z = parseInt(prompt());
         var aa = parseInt(prompt());
         var ba = parseInt(prompt());



         document.write("product of matrices= ");
         console.log(document.write( + a * m + b * q + c * u + d * y + ','));
         console.log(document.write( + a * n + b * r + c * v + d * z + ','));
         console.log(document.write( + a * o + b * s + c * w + d * aa + ','));
         console.log(document.write( + a * p + b * t + c * x + d * ba + ','));
         ;
         console.log(document.write( + e * m + f * q + g * u + h * y + ','));
         console.log(document.write( + e * n + f * r + g * v + h * z + ','));
         console.log(document.write( + e * o + f * s + g * w + h * aa + ','));
         console.log(document.write( + e * p + f * t + g * x + h * ba + ','));
         ;
         console.log(document.write( + i * m + j * q + k * u + l * y + ','));
         console.log(document.write( + i * n + j * r + k * v + l * z + ','));
         console.log(document.write( + i * o + j * s + k * w + l * aa + ','));
         console.log(document.write( + i * p + j * t + k * x + l * ba + ','));


      }

      // multiply 4x4 & 4x4
      else if(rowA == 4 && columnA == 4 && rowB == 4 && columnB == 4)
      {
         var a = parseInt(prompt());
         var b = parseInt(prompt());
         var c = parseInt(prompt());
         var  d = parseInt(prompt());
         var e = parseInt(prompt());
         var f = parseInt(prompt());
         var g = parseInt(prompt());
         var h = parseInt(prompt());
         var i = parseInt(prompt());
         var j = parseInt(prompt());
         var k = parseInt(prompt());
         var l = parseInt(prompt());
         //
         var m = parseInt(prompt());
         var n = parseInt(prompt());
         var o = parseInt(prompt());
         var p = parseInt(prompt());
         var q = parseInt(prompt());
         var r = parseInt(prompt());
         var s = parseInt(prompt());
         var t = parseInt(prompt());
         var u = parseInt(prompt());
         var v = parseInt(prompt());
         var w = parseInt(prompt());
         var x = parseInt(prompt());
         var y = parseInt(prompt());
         var z = parseInt(prompt());
         var aa = parseInt(prompt());
         var ba = parseInt(prompt());
         var ca = parseInt(prompt());
         var da = parseInt(prompt());
         var ea = parseInt(prompt());
         var fa = parseInt(prompt());


         document.write("product of matrices= ");
         console.log(document.write( + a * q + e * r + i * s + m * t + ','));
         console.log(document.write( + a * u + e * v + i * w + m * x + ','));
         console.log(document.write( + a * y + e * z + i * aa + m * ba + ','));
         console.log(document.write( + a * ca + e * da + i * ea + m * fa + ','))
         ;
         console.log(document.write( + b * q + f * r + j * s + n * t + ','));
         console.log(document.write( + b * u + f * v + j * w + n * x + ','));
         console.log(document.write( + b * y + f * z + j * aa + n * ba + ','));
         console.log(document.write( + b * ca + f * da + j * ea + n * fa + ','))
         ;
         console.log(document.write( + c * q + g * r + k * s + o * t + ','));
         console.log(document.write( + c * u + g * v + k * w + o * x + ','));
         console.log(document.write( + c * y + g * z + k * aa + o * ba + ','));
         console.log(document.write( + c * ca + g * da + k * ea + o * fa + ','))
         ;
         console.log(document.write( + d * q + h * r + l * s + p * t + ','));
         console.log(document.write( + d * u + h * v + l * w + p * x + ','));
         console.log(document.write( + d * y + h * z + l * aa + l * ba + ','));
         console.log(document.write( + d * ca + h * da + l * ea + p * fa + ','));

      }



   }
}




summator.matx();

